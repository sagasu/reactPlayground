{
  "name": "react-playground",
  "version": "1.0.0",
  "description": "playground for various react stuff",
  "main": "index.js",
  "scripts": {
    "test-simple": "mocha ./simple/add.spec.js",
    "test-chai": "mocha ./chaiUsed/add.spec.js",
    "test-helloWorldHarmony": "mocha  --harmony --harmony-proxies ./helloWorld/HelloWorld.spec.js",
    "test-helloWorld": "mocha --compilers js:babel-register ./helloWorld/HelloWorld.spec.js",
    "test-helloWorldChaiJsx": "mocha --compilers js:babel-register ./helloWorldChaiJsx/HelloWorld.spec.js",
    "test-helloWorldEnzyme": "mocha --compilers js:babel-register ./helloWorldEnzyme/HelloWorld.spec.js"
  },
