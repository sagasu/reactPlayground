  "devDependencies": {
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.3.0",
    "jsx-chai": "^4.0.0",
    "mocha": "^2.5.3",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.1.0",
